{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "parkings",
  "version": "0.1.0",
  "identifier": "com.parkings.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173/Parking_bmstu_frontend/",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  
  "plugins": {
        "http": {
          "enabled": true
        }
    },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "parkings",
        "width": 800,
        "height": 600,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}